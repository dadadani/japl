
//[Test: fibonacci]

//[source: raw]
fun fib(n) {
    if (n < 2)
        return n;
    return fib(n-2) + fib(n-1);
}
print(fib(1));
print(fib(2));
print(fib(3));
print(fib(4));
print(fib(5));
print(fib(6));
print(fib(7));
print(fib(8));
print(fib(9));
